{
    "headers": [
        {
        "source": "/(.*)",
            "headers": [
                { "key": "Cache-Control", "value": "no-store, no-cache, must-revalidate, proxy-revalidate" },
                { "key": "Pragma", "value": "no-cache" },
                { "key": "Expires", "value": "0" }
            ]   
        }
    ],
    "version": 2,
    "builds": [
        {
        "src": "server.js", 
        "use": "@vercel/node"
        }
    ],
    "routes": [
        {
        "src": "/(.*)",
        "dest": "server.js"
        }
    ]
}